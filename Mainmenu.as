package  {		import flash.events.MouseEvent;	import flash.events.TouchEvent;	import flash.ui.Multitouch;	import flash.ui.MultitouchInputMode;		import flash.events.Event;			import flash.display.MovieClip;	import flash.events.Event;	import com.greensock.TweenLite;	import com.greensock.*;	import com.greensock.easing.*;			public class Mainmenu extends MovieClip {				var mymain:Main				public function Mainmenu(inMain) {			// constructor code			mymain = inMain			showDrawingPreviews()		}						//make array of drawings, loop though them and display,				private function showDrawingPreviews() {			var myTimeline:TimelineLite = new TimelineLite(); 			var thumb = new Thumbnail(this,mymain,new Horsedrawing(),0)			thumb.x = 50			thumb.y = 800			addChild(thumb)			myTimeline.append(TweenLite.to(thumb, 0.5, {y:400, ease:Elastic.easeOut}));						var thumb = new Thumbnail(this,mymain,new Cardrawing(),1)			thumb.x = 300			thumb.y = 800			addChild(thumb)			myTimeline.append(TweenLite.to(thumb, 0.5, {y:400, ease:Elastic.easeOut}));									var thumb = new Thumbnail(this,mymain,new Rocketdrawing(),2)			thumb.x = 550			thumb.y = 800			addChild(thumb)			myTimeline.append(TweenLite.to(thumb, 0.5, {y:400, ease:Elastic.easeOut}));									var thumb = new Thumbnail(this,mymain,new Dolphindrawing(),3)			thumb.x = 800			thumb.y = 800			addChild(thumb)			myTimeline.append(TweenLite.to(thumb, 0.5, {y:400, ease:Elastic.easeOut}));																	}				public function show() {			x=-300			visible = true			mymain.gameState = "mainmenu"			TweenLite.to(this, 2, {x:0, ease:Bounce.easeOut});					}				public function hide() {			visible = false			mymain.gameState = "coloring"		}					}	}