package  {		import flash.events.MouseEvent;	import flash.events.TouchEvent;	import flash.ui.Multitouch;	import flash.ui.MultitouchInputMode;		import flash.events.Event;			import flash.display.MovieClip;	import flash.events.Event;	import com.greensock.TweenLite;	import com.greensock.*;	import com.greensock.easing.*;			public class Mainmenu extends MovieClip {				var mymain:Main				public function Mainmenu(inMain) {			// constructor code			mymain = inMain			showDrawingPreviews()		}						//make array of drawings, loop though them and display,				private function showDrawingPreviews() {			var thumb = new Thumbnail(this,mymain,new Horsedrawing(),0)			thumb.x = 0			thumb.y = 200			addChild(thumb)			TweenLite.to(thumb, 2, {x:50, ease:Elastic.easeOut});			var thumb = new Thumbnail(this,mymain,new Cardrawing(),1)			thumb.x = 300			thumb.y = 200			addChild(thumb)						var thumb = new Thumbnail(this,mymain,new Rocketdrawing(),2)			thumb.x = 50			thumb.y = 400			addChild(thumb)						var thumb = new Thumbnail(this,mymain,new Dolphindrawing(),3)			thumb.x = 300			thumb.y = 400			addChild(thumb)														}				public function show() {			x=-300			visible = true			mymain.gameState = "mainmenu"			TweenLite.to(this, 1, {x:0, ease:Elastic.easeOut});					}				public function hide() {			visible = false			mymain.gameState = "coloring"		}					}	}